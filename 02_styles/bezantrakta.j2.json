{
    "css": {
        "#signature": {
            "border-collapse": "collapse",
            "color": "#000000",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "16px",
            "font-style": "normal",
            "font-weight": "normal",
            "text-decoration": "none",
            "margin": "0"
        },
        "table, table, table tr td": {
            "border": "none",
            "padding": "0",
            "white-space": "nowrap"
        },
        "img": {
            "margin": "0",
            "vertical-align": "middle"
        },
        ".name": {
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "14px",
            "font-weight": "bold",
            "padding": "0 4px"
        },
        ".occupation": {
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "14px",
            "font-weight": "normal",
            "padding": "0 4px"
        },
        ".belcanto-text": {
            "color": "#000000",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "12px"
        },
        ".bezantrakta-text": {
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "16px",
            "font-weight": "bold"
        },
        ".icon": {
            "border-width": "1px",
            "border-style": "solid",
            "border-color": "#000",
            "border-radius": "6px",
            "margin": "1px"
        },
        "a, a:link, a:hover, a:visited": {
            "color": "#000000",
            "text-decoration": "none",
            "padding": "0 2px"
        },
        ".grey-text": {
            "color": "#707070",
            "font-family": "Arial, Helvetica, sans-serif",
            "text-decoration": "none",
            "padding": "0 2px"
        },
        ".small-text": {
            "display": "block",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "12px",
            "padding": "0 3px",
            "letter-spacing": "0.2px"
        },
        ".telephone": {
            "color": "#000000",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "16px"
        },
        ".text-link": {
            "padding": "4px 0"
        },
        ".center": {
            "text-align": "center"
        },
        ".motto": {
            "font-size": "14px"
        }
    }
}