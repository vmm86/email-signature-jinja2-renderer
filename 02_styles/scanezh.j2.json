{
    "css": {
        "#signature": {
            "border-collapse": "collapse",
            "color": "#000000",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "16px",
            "font-style": "normal",
            "font-weight": "normal",
            "text-decoration": "none",
            "margin": "0"
        },
        "table, table, table tr td": {
            "border": "none",
            "padding": "0",
            "padding-bottom": "1px",
            "white-space": "nowrap"
        },
        "img": {
            "margin": "0",
            "vertical-align": "middle"
        },
        "a, a:link, a:hover, a:visited": {
            "color": "inherit",
            "text-decoration": "none",
            "padding": "2px 0"
        },
        ".grey-text": {
            "color": "#000000",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "16px",
            "text-decoration": "none",
            "padding": "2px 0"
        },
        ".grey-text": {
            "color": "#97999B",
            "font-family": "Arial, Helvetica, sans-serif",
            "font-size": "12px",
            "text-decoration": "none",
            "padding": "2px 0"
        },
        ".center": {
            "text-align": "center"
        }
    }
}